      "post": {
        "tags": ["Front Api Orders"],
        "summary": "Create checkout order",
        "operationId": "7ad6ee71e83f7bd63eead9dd9b43a307",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": ["shipping_address", "name", "email", "phone", "payment_gateway"],
                "properties": {
                  "shipping_address": {
                    "properties": {
                      "country": {
                        "type": "string",
                        "example": "Egypt"
                      },
                      "state": {
                        "type": "string",
                        "example": "Cairo"
                      },
                      "city": {
                        "type": "string",
                        "example": "Nasr City"
                      },
                      "address": {
                        "type": "string",
                        "example": "Street 123"
                      }
                    },
                    "type": "object"
                  },
                  "name": {
                    "type": "string",
                    "example": "John Doe"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "john.doe@example.com"
                  },
                  "phone": {
                    "type": "string",
                    "example": "01010101010"
                  },
                  "payment_gateway": {
                    "type": "string",
                    "enum": [
                      "cash_on_site",
                      "cash_on_delivery",
                      "fawaterk_gateway",
                      "send_receipt",
                      "paymob_card_gateway",
                      "paymob_wallet_gateway"
                    ],
                    "example": "cash_on_delivery"
                  },
                  "coupon_code": {
                    "type": "string",
                    "example": "Sfa35",
                    "nullable": true
                  },
                  "receipt_image": {
                    "type": "string",
                    "example": "path/to/image.jpg",
                    "nullable": true
                  },
                  "wallet_number": {
                    "type": "string",
                    "example": "01010101010",
                    "nullable": true
                  }
                },
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Order created successfully"
          },
          "401": {
            "description": "Unauthenticated"
          },
          "422": {
            "description": "Validation error"
          }
        },
        "security": [
          {
            "apiAuth": []
          },
          {
            "guestToken": []
          }
        ]
      }
    },
